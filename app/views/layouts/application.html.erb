<!DOCTYPE html>
<html>
  <head>
    <title>ILoveForms</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width-device-width, initial-scale=1">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>

  </head>

  <body>
    <div class="container-fluid">
      <ul class ="navbar navbar-expand-lg navbar-light >
        <li class="navbar">
          <a href="<%= home_path %>" onclick="ga('send', 'event', 'Navbar', 'Community links', 'Bootstrap');">Home</a>
        </li>
        <li class="navbar">
          <a href="<%= new_path %>" onclick="ga('send', 'event', 'Navbar', 'Community links', 'Docs');">New User</a>
        </li>
      </ul>
    </div>
    <div class="container">
    <%= yield %>
    </div>
  </body>
</html>
